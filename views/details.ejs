<% layout('/layouts/biolerplate.ejs') -%>
<body>
<div class="table-responsive col-12 col-sm-10 col-md-8 col-lg-6 mx-auto mt-4 mb-5 p-4 border rounded-3 shadow">
    
    <table class="table table-striped table-hover">
        <caption class="text-center fs-5 mb-2">
        <h2 id="particpantdetails"><b>Participants Details</b></h2></caption>
   
   <tr>
    
    <th class="d-none d-sm-table-cell">Name</th>
    <th class="d-none d-sm-table-cell">College</th>
    <th  class="d-none d-sm-table-cell">Branch</th>
    <th class="d-none d-md-table-cell">Year</th>
    <th class="d-none d-md-table-cell">RollNo</th>
    <th class="d-none d-sm-table-cell">Phone</th>
    <th class="d-none d-sm-table-cell">Email</th>
   </tr>
   
   <% for(res of result){ %>
    
    <tr>
        
        <td class="d-none d-sm-table-cell"><%=res.name%></td>
        <td class="d-none d-sm-table-cell"><%=res.college%></td>
        <td class="d-none d-sm-table-cell"><%=res.branch%></td>
        <td class="d-none d-md-table-cell"><%=res.year%></td>
        <td class="d-none d-md-table-cell"><%=res.rollno%></td>
        <td class="d-none d-sm-table-cell"><%=res.phone%></td>
        <td class="d-none d-sm-table-cell"><%=res.email%></td>
    </tr>
    
    <% } %>
</table>
</div>
</body>

